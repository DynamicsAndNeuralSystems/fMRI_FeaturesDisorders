--data_path ${data_path} \
--rdata_path ${rdata_path} \
--github_dir ${github_dir} \
--pairwise \
--num_k_folds 10 \
--output_data_dir /headnode1/abry4213/data/UCLA_CNP_ABIDE_ASD/processed_data/Rdata/UCLA_CNP_ABIDE_ASD_SPI_wise_pyspi14_inv_prob_null_model_fits/ \
--null_iter_number 1 \
--num_perms_for_iter 1 \
--pairwise_feature_set pyspi14 \
--univariate_feature_set catch22 \
--svm_kernel linear \
--grouping_var SPI \
--svm_feature_var region_pair \
--use_inv_prob_weighting \
--weighting_name inv_prob

exit

# Verify the host on which the job ran
hostname
